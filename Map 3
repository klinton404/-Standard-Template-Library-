#include<bits/stdc++.h>
using namespace std;
int main()
{
    map<int,int>mp;
    mp[0]=10;
    mp.insert({1,20});
    mp.insert({2,30});
    mp.insert({3,40});
    mp.insert({4,50});

    map<int, int>::iterator it;
    for(it=mp.begin();  it!=mp.end();  it++)
    {
        cout<<it->first <<" " <<it->second <<"\n";
    }
}
