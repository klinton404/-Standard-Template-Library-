#include<bits/stdc++.h>
using namespace std;
int main()
{
    map<int,int>mp;
    mp[0]=10;
    mp.insert({1,20});
    mp.insert({2,30});
    mp.insert({3,40});
    mp.insert({4,50});


    auto it = mp.lower_bound(4);

    cout<< (*it).first <<" " <<(*it).second <<"\n";  
    //Print immediate Upper Value
}


