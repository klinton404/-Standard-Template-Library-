#include<bits/stdc++.h>
using namespace std;

int main()
{
    set<int>s;


    s.insert(1);
    s.insert(2);
    s.insert(4);
    s.insert(3);
    s.insert(5);
    s.insert(1);    // It allow only single times

    cout<<s.size() <<"\n";
    cout<<s.max_size() <<"\n";

    cout<<"\n";


    set<int>::iterator it;
    for(it=s.begin();   it!=s.end();   it++)
    {
        cout  <<*it  <<" " <<"\n";   // Assending
    }
}
