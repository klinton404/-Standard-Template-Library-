#include<bits/stdc++.h>
using namespace std;

int main()
{
    set<int>s;


    s.insert(1);
    s.insert(2);
    s.insert(4);
    s.insert(3);
    s.insert(5);
    s.insert(6);
    s.insert(7);
    s.insert(1);    // It allow only single times


    s.erase(2);  // erase 2

    set<int>::iterator it;
    it = s.begin();    // erase begin

    for(auto it: s)
    {
        cout<<it<<" " <<"\n";
    }
}
